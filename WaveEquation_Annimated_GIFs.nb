Cell["\<\
Produces animated GIFs for the solution of the 1D wave equation.
Assumes L = c = 1.\
\>", "Text",
 CellChangeTimes->{{3.8123500614628067`*^9, 3.812350077020293*^9}, {
  3.812350127392229*^9, 
  3.812350132394183*^9}},ExpressionUUID->"77015db7-ea01-481a-a779-\
2403103d9298"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"SetDirectory", "[", 
   RowBox[{"NotebookDirectory", "[", "]"}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"anCoefs", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"nmax_:", "100"}]}], "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"2", " ", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "[", "x", "]"}], " ", 
        RowBox[{"Sin", "[", 
         RowBox[{"n", " ", "\[Pi]", " ", "x"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"n", ",", "1", ",", "nmax"}], "}"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bnCoefs", "[", 
   RowBox[{"g_", ",", 
    RowBox[{"nmax_:", "100"}]}], "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{
     FractionBox["2", 
      RowBox[{"n", " ", "\[Pi]"}]], " ", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"g", "[", "x", "]"}], " ", 
        RowBox[{"Sin", "[", 
         RowBox[{"n", " ", "\[Pi]", " ", "x"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", "1"}], "}"}]}], "]"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"n", ",", "1", ",", "nmax"}], "}"}]}], "]"}]}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.8123500812320757`*^9, 3.812350200710127*^9}, 
   3.812350341671701*^9, {3.812350591836429*^9, 3.8123505920624437`*^9}, {
   3.812350721828684*^9, 3.812350725732539*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"59149de6-3308-4627-b677-63efacf036d5"],

Cell[CellGroupData[{

Cell["Example", "Subsubsection",
 CellChangeTimes->{{3.812350208153234*^9, 3.812350218159418*^9}, 
   3.81235068075*^9},ExpressionUUID->"fa15372b-5d35-4d8a-af12-5cc108058c0b"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{"x", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["x", "3"], "-", 
      RowBox[{"2", 
       SuperscriptBox["x", "2"]}], "+", "1"}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "x_", "]"}], ":=", " ", 
   RowBox[{"x", 
    RowBox[{"(", 
     RowBox[{"1", "-", "x"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"an", " ", "=", " ", 
   RowBox[{
    RowBox[{"anCoefs", "[", 
     RowBox[{"f", ",", "100"}], "]"}], "//", "Quiet"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"bn", " ", "=", " ", 
    RowBox[{
     RowBox[{"bnCoefs", "[", 
      RowBox[{"g", ",", "100"}], "]"}], "//", "Quiet"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "I", " ", "use", " ", "this", " ", "function", " ", "\"\<Compile\>\"", " ",
     "to", " ", "make", " ", "the", " ", "evaluation", " ", "of", " ", "u", 
    " ", "faster"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"u", " ", "=", " ", 
   RowBox[{"Compile", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x", ",", "_Real"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "_Real"}], "}"}]}], "}"}], ",", 
     RowBox[{"Total", "@", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{
            "an", "\[LeftDoubleBracket]", "n", "\[RightDoubleBracket]"}], " ", 
            RowBox[{"Cos", "[", 
             RowBox[{"n", " ", "\[Pi]", " ", "t"}], "]"}]}], " ", "+", " ", 
           RowBox[{
            RowBox[{
            "bn", "\[LeftDoubleBracket]", "n", "\[RightDoubleBracket]"}], " ", 
            RowBox[{"Sin", "[", 
             RowBox[{"n", " ", "\[Pi]", " ", "t"}], "]"}]}]}], ")"}], 
         RowBox[{"Sin", "[", 
          RowBox[{"n", " ", "\[Pi]", " ", "x"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "1", ",", 
          RowBox[{"Length", "[", "an", "]"}]}], "}"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.812350219843769*^9, 3.812350256129726*^9}, {
